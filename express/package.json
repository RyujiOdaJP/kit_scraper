{
  "name": "kit_scraper_express",
  "version": "1.0.0",
  "description": "RPA as learning TypeScript/Express https://www.kit-victims.org/",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "ts-node ./build.ts && tsc --lib es2020",
    "dev": "nodemon ./server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec 'node -r ./ts-node/register --inspect-brk' ./server/index.ts | pino-pretty",
    "lint": "tsc --noEmit && eslint './server/**/*.{js,ts,tsx}' './test/**/*.{js,ts,tsx}' './*.{js,ts,tsx}' './.*.{js,ts,tsx}' --config ./.eslintrc.js --no-ignore --cache --color --fix",
    "test": "mocha test/**/*.ts --exit",
    "test:debug": "mocha --inspect-brk ./test/**/*.ts --exit"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^3.12.7",
    "fs-extra": "^9.0.1",
    "helmet": "^3.22.0",
    "pino": "^6.2.1",
    "swagger-ui-express": "^4.1.4",
    "tsoa": "^2.5.14"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/pino": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "nodemon": "^2.0.3",
    "pino-pretty": "^4.0.0",
    "prettier": "^2.0.5",
    "shelljs": "^0.8.4",
    "supertest": "^4.0.2",
    "ts-lint": "^4.5.1",
    "ts-node": "^8.9.1",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
  },
  "author": "Carmine DiMascio <cdimascio@gmail.com> (https://github.com/cdimascio)"
}
